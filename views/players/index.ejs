
<% layout('layout/boilerplate.ejs') %>
<div class="index">
<% for(let s of song) {%>
    <div class="item">
        <div class="image">
            <img src="<%= s.songImg %>" alt="">
        </div>
        <div class="text">
        <h2><a href="/play/<%= s._id %>"><%= s.name %></a></h2>
        </div>
        <h3>By <%= s.singer %></h3>
    </div>
<% } %>
</div>
